{
  "name": "@optbot/eqpub",
  "version": "0.0.0",
  "description": "publish active equities to a queue",
  "main": "lib/norequire.js",
  "scripts": {
    "setup": "node lib/setup.js",
    "start": "node lib/daemon.js --action start",
    "stop": "node lib/daemon.js --action stop",
    "test": "./node_modules/.bin/mocha"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/optbot/eqpub.git"
  },
  "keywords": [
    "eqpub",
    "optbot"
  ],
  "author": "Marshall Farrier",
  "license": "MIT",
  "files": [
    "AUTHORS",
    "LICENSE",
    "README.md",
    "lib/"
  ],
  "config": {
    "user": "eqpub",
    "service": "eqpub.py",
    "command": "/usr/bin/python",
    "serviceDir": "src"
  },
  "dependencies": {
    "@optbot/daemon-setup": "git://github.com/optbot/daemon-setup.git",
    "@optbot/py-daemon": "git://github.com/optbot/py-daemon.git#marshall",
    "yargs": "~3.10.0"
  },
  "devDependencies": {
    "chai": "~2.2.0",
    "jscs": "~1.12.0",
    "jshint": "~2.7.0",
    "mocha": "~2.2.4"
  }
}
